{
  "name": "seatmap-backend",
  "version": "1.0.0",
  "description": "Backend API for Seatmap Management System",
  "main": "src/app.js",
  "scripts": {
    "start": "NODE_ENV=production node src/app.js",
    "dev": "nodemon src/app.js",
    "build": "echo 'Backend build completed'",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop seatmap-backend",
    "pm2:restart": "pm2 restart seatmap-backend",
    "migrate": "sequelize-cli db:migrate",
    "migrate:prod": "NODE_ENV=production sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "create-migration": "sequelize-cli migration:generate --name",
    "create-model": "sequelize-cli model:generate --name"
  },
  "keywords": [
    "seatmap",
    "venue",
    "seats",
    "management"
  ],
  "author": "Seatmap Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.2.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^6.0.0",
    "express-session": "^1.18.2",
    "express-validator": "^6.14.0",
    "helmet": "^7.0.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.0",
    "qrcode": "^1.5.4",
    "sequelize": "^6.35.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
